{
  "name": "leximaven",
  "version": "4.0.0",
  "description": "A command line tool for searching word-related APIs.",
  "author": "Andrew Prentice",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "bin/leximaven.js",
  "bin": {
    "leximaven": "bin/leximaven.js"
  },
  "scripts": {
    "bin": "cross-env BABEL_ENV=dev babel src -d bin",
    "bump": "git push origin master --tags && npm publish",
    "instrument": "cross-env BABEL_ENV=test babel src -d bin",
    "lint": "standard --fix src",
    "release": "standard-version",
    "test": "echo 'No tests.'",
    "watch": "babel -w src -d bin"
  },
  "dependencies": {
    "ansi-colors": "^3.2.3",
    "date-fns": "^1.30.1",
    "dot-prop": "^4.2.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "good-guy-http": "^1.12.0",
    "lodash": "^4.17.11",
    "noon": "^1.5.0",
    "ora": "^3.0.0",
    "random-word": "^2.0.0",
    "scrape-it": "5.1.4",
    "term-size": "^1.2.0",
    "wrap-ansi": "^4.0.0",
    "xml2js": "^0.4.19",
    "yargonaut": "^1.1.4",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "has-ansi": "^3.0.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "standard-version": "^4.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drawnepicenter/leximaven.git"
  },
  "bugs": {
    "url": "https://github.com/drawnepicenter/leximaven/issues"
  },
  "homepage": "https://github.com/drawnepicenter/leximaven#readme",
  "keywords": [
    "lyracyst",
    "lyricist",
    "leximaven",
    "cli",
    "word",
    "search",
    "api",
    "scraper",
    "rest",
    "anagram",
    "acronym",
    "define",
    "dictionary",
    "thesaurus",
    "slang",
    "rhyme",
    "pronunciation"
  ]
}
